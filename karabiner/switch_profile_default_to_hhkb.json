{
  "title": "switch_profile_to_hhkb",
  "rules": [
    {
      "description": "switch_profile_to_hhkb",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "insert"
          },
          "to": [
            {
              "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'HHKB profile'"
            }
          ]
        }
      ]
    }
  ]
}
